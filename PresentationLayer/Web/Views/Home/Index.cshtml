@model IEnumerable<ContactModel>

<div class="row">
    <div class="separator"></div>

    <div class="tableHeaderContainer">
        <div class="tableHeader">Name</div>
        <div class="tableHeader">Date of birth</div>
        <div class="tableHeader">Picture</div>
    </div>

        @foreach (var contact in Model)
        {
            <a href="Details/@contact.Id"><div class="tableContainer">
                <div class="tableColl">@contact.Name</div>
                <div class="tableColl">@contact.DateOfBirth</div>
                <div class="tableColl">@contact.Picture</div>
            </div></a>
        }

    </div>